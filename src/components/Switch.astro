---
interface Props {
  key: string;
}

function match<T>(key: string, cases: Record<string, () => T>) {
  return cases[key]?.();
}

const { key } = Astro.props;
---

{/* slot[name] must be a static string */}
{
  match(key, {
    oc: () => <slot name="oc" />,
    faculty_members: () => <slot name="faculty_members" />,
    utokyo_account: () => <slot name="utokyo_account" />,
    courses: () => <slot name="courses" />,
  })
}
