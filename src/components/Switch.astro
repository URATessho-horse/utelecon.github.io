---
import type { Variant } from "./types";

interface Props {
  variant: Variant;
}

function match<T>(key: string, cases: Record<string, () => T>) {
  return cases[key]?.();
}

const { variant } = Astro.props;
---

{/* slot[name] must be a static string */}
{
  match(variant, {
    oc: () => <slot name="oc" />,
    faculty_members: () => <slot name="faculty_members" />,
    individual: () => <slot name="individual" />,
  })
}
